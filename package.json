{
  "name": "fua-pwa",
  "version": "1.0.0-alpha",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "deploy": "npm run build && firebase deploy",
    "build": "ng build --prod --aot",
    "build.stg": "ng build --configuration staging",
    "build.prod": "ng build --configuration production",
    "prebuild": "npm rebuild node-sass",
    "precache": "sw-precache --verbose --config=sw-precache-config.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start.http": "ng build --prod && http-server ./dist -o -p 4101",
    "serve.http": "http-server ./dist -o -p 4101",
    "start.https": "ng build --prod && http-server ./dist -S -o -p 4102",
    "serve.https": "http-server ./dist -S -o -p 4102",
    "createkey": "openssl req -x509 -newkey rsa:4096 -keyout key.pem -out cert.pem -nodes -days 365 -subj '/CN=localhost'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.11",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "^8.2.11",
    "@angular/compiler": "^8.2.11",
    "@angular/core": "^8.2.11",
    "@angular/forms": "^8.2.11",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.11",
    "@angular/platform-browser-dynamic": "^8.2.11",
    "@angular/platform-server": "^8.2.11",
    "@angular/pwa": "^0.803.9",
    "@angular/router": "^8.2.11",
    "@angular/service-worker": "^8.2.11",
    "@auth0/angular-jwt": "^3.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "animate.css": "^3.7.2",
    "fastclick": "^1.0.6",
    "ngx-device-detector": "^1.3.11",
    "reset-css": "^5.0.0",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.2.0",
    "@angular-devkit/build-angular": "^0.803.9",
    "@angular/cli": "8.0.1",
    "@angular/compiler-cli": "^8.2.11",
    "@types/jasmine": "3.4.4",
    "@types/node": "~12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "merge-jsons-webpack-plugin": "^1.0.20",
    "protractor": "~5.4.2",
    "ts-node": "~8.4.1",
    "tslint": "^5.20.0",
    "typescript": "~3.5.3"
  }
}
